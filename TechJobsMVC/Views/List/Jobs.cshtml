
<div class="container body-content">

    <h1>@ViewBag.title</h1>

    @{
        //TODO #1: Use a loop to display job results in a table.

   <table class="job-listing">

    @foreach(var j in ViewBag.jobs)
    {   <tr>
            <th>ID: @j.Id</th>
            <th>Name: @j.Name</th>
            <th>Employer: @j.Employer</th>
            <th>Location: @j.Location</th>
            <th>Position Type: @j.PositionType</th>
            <th>Core Competency: @j.CoreCompetency</th>
         </tr>
    }
    </table>


    }


</div>